{
  "config": {
    "default": "axg-101"
  },
  "axg-101": {
    "config": {
      "title": "Appxigon 101 Talk Slides",
      "version": "0.1.0",
      "vars": null,
      "data": null,
      "class": {
        "appxigon": "container",
        "view": null,
        "group": null,
        "item": null
      },
      "default": "slide-cover",
      "layout": [
        "header",
        "_main",
        "footer"
      ],
      "signal": {
        "Enter": {
          "uri": ".nextView",
          "params": {
            "step": 1,
            "views": [
              "slide-cover",
              "slide-speakers",
              "slide-intro"
            ]
          }
        },
        "ArrowRight": {
          "uri": ".nextView",
          "params": {
            "step": 1,
            "views": [
              "slide-cover",
              "slide-speakers",
              "slide-intro"
            ]
          }
        },
        "ArrowLeft": {
          "uri": ".nextView",
          "params": {
            "step": -1,
            "views": [
              "slide-cover",
              "slide-speakers",
              "slide-intro"
            ]
          }
        }
      },
      "view": {
        "actions": [
          {
            "call": {
              "uri": ".transitClass",
              "params": {
                "name": "view-transition-effect",
                "timeout": 2000
              }
            }
          }
        ]
      }
    },
    "slide-cover": {
      "config": {
        "title": null,
        "layout": [
          "main",
          "speakers"
        ]
      },
      "main": [
        {
          "logo": {
            "type": "display",
            "value": "div",
            "class": "logo"
          }
        },
        {
          "content": {
            "type": "display",
            "content": "Less is more\n============\nBeyond Technical Frameworks\n"
          }
        }
      ]
    },
    "slide-speakers": {
      "config": {
        "layout": [
          "main",
          "footnote"
        ]
      },
      "main": [
        {
          "content": {
            "type": "display",
            "content": "Who are we?\n============\n"
          }
        },
        {
          "speakers": {
            "class": "speakers-list",
            "type": "select/list",
            "data": {
              "default": [
                {
                  "value": "roger",
                  "text": "Roger LAM"
                },
                {
                  "value": "siegfried",
                  "text": "Siegfried GRIMBEEK"
                }
              ]
            }
          }
        }
      ]
    },
    "slide-intro": {
      "config": {
        "layout": [
          "main",
          "footnote"
        ]
      },
      "main": [
        {
          "content": {
            "type": "display",
            "content": "What to expect:\n============\nBeyond Technical Frameworks\n"
          }
        },
        {
          "supplementary": {
            "type": "display",
            "content": "Yada yada...\n----------\nBeyond the horizon ...\n"
          }
        }
      ],
      "footnote": [
        {
          "content": {
            "type": "display",
            "content": "Some footnote here...\n"
          }
        }
      ]
    },
    "slide-intro-less-is-more": null,
    "slide-problem": null,
    "slide-proposal": null,
    "slide-future": null,
    "slide-conclusion": null,
    "feedback": {
      "config": {
        "title": null,
        "layout": [
          "main",
          "misc"
        ]
      },
      "main": [
        {
          "audience-feedback-input": {
            "type": "emoji-input"
          }
        },
        {
          "audience-feedback-display": {
            "type": "emoji-display"
          }
        }
      ],
      "misc": [
        {
          "statistics": {
            "type": "display"
          }
        }
      ]
    },
    "header": {
      "footer": null,
      "config": {
        "class": "custom-footer-view",
        "layout": [
          "default"
        ]
      },
      "default": [
        {
          "config": {
            "class": "custom-footer-group"
          }
        },
        {
          "logo": {
            "type": "response-form"
          }
        }
      ]
    }
  }
}